{
  "name": "@mockforge/forgeconnect",
  "version": "0.1.0",
  "description": "MockForge ForgeConnect - Browser SDK for intercepting requests and creating mocks from the browser",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SaaSy-Solutions/mockforge.git",
    "directory": "sdk/browser"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run test:unit"
  },
  "keywords": [
    "mock",
    "testing",
    "api",
    "http",
    "browser",
    "intercept",
    "forgeconnect"
  ],
  "author": "MockForge Contributors",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "jsdom": "^23.0.0",
    "rollup": "^4.9.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}

