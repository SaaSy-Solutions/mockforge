{
  "description": "Valid API key verification response",
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "valid": true,
      "key_id": "key_{{random_string 12}}",
      "owner_id": "org_{{random_string 8}}",
      "scopes": ["read", "write"],
      "rate_limit": {
        "limit": 1000,
        "remaining": 999,
        "reset_at": "{{math (now 'unix') '+' 3600}}"
      },
      "created_at": "2024-01-01T00:00:00Z",
      "last_used_at": "{{now 'iso'}}"
    }
  }
}
